<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activities.meniuDoctor.retetaNoua">

    <include
        android:id="@+id/include4"
        layout="@layout/back_toolbar" />

    <LinearLayout
        android:layout_width="412dp"
        android:layout_height="482dp"
        android:layout_marginTop="48dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include4">

        <TextView
            android:id="@+id/textView23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ID User" />

        <EditText
            android:id="@+id/etUserReteta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="List cu ID medicamente (separate prin virgula)" />

        <EditText
            android:id="@+id/etIdMedicamente"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Lista cu nr medicamente, in aceeasi ordine (separate prin virgula)" />

        <EditText
            android:id="@+id/etNrMedicamente"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Frecventa, in aceeasi ordine (separate prin virgula)" />

        <EditText
            android:id="@+id/etFrecventaMedicamente"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="" />

        <Button
            android:id="@+id/buttonTrimiteReteta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Trimite reteta" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>